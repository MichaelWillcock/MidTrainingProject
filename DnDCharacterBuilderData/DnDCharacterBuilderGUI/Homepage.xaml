<Page x:Class="DnDCharacterBuilderGUI.Homepage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DnDCharacterBuilderGUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Homepage">

    <Grid>
        <Button Content="Logout" HorizontalAlignment="Center" Margin="0,164,0,0" VerticalAlignment="Top" Click="Logout_Click"/>
        <TextBlock Name="Welcome1" HorizontalAlignment="Left" Margin="268,274,0,0" Text="{Binding textdata}" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock x:Name="Welcome" Margin="268,274,0,0">
            <Run Text="Insert Text"/>
            <Run Text="{Binding textdata}"/>
        </TextBlock>
    </Grid>
</Page>
